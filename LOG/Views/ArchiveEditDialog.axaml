<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="225"
        x:Class="LOG.Views.ArchiveEditDialog"
        Title="AddArchiveDialog"
        Width="400" Height="225">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        
        <TextBlock Text="请输入名字"></TextBlock>
        
        <TextBox x:Name="Name" Grid.Row="1" Watermark="Name"></TextBox>

        <Button Grid.Row="2" Content="Sure" HorizontalAlignment="Center" Click="Close_Click"></Button>
        
    </Grid>
</Window>
